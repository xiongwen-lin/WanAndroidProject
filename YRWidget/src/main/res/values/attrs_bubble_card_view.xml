<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleCardView">
        <attr name="backgroundColor" format="color" />
        <attr name="triangleOrientation" format="integer" >
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="triangleRadius" format="integer" />
    </declare-styleable>
</resources>