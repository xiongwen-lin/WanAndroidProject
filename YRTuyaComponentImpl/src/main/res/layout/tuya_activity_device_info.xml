<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingStart="@dimen/dp_10"
    android:paddingEnd="@dimen/dp_10">

    <com.apemans.quickui.label.LabelTextInfoIconView
        android:id="@+id/label_model"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />

    <com.apemans.quickui.label.LabelTextInfoIconView
        android:id="@+id/label_device_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/label_model" />

    <com.apemans.quickui.label.LabelTextInfoIconView
        android:id="@+id/label_ip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/label_device_id" />

    <com.apemans.quickui.label.LabelTextInfoIconView
        android:id="@+id/label_mac"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/label_ip" />

    <!--    <com.apemans.quickui.label.LabelTextIconView-->
    <!--        android:id="@+id/label_contact_us"-->
    <!--        android:layout_width="match_parent"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        app:layout_constraintTop_toBottomOf="@id/label_firmware" />-->

</androidx.constraintlayout.widget.ConstraintLayout>