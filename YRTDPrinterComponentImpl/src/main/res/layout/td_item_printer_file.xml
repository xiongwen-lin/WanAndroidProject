<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/vPrintFile"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/td_printer_file_background"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_80"/>

    <ImageView
        android:id="@+id/ivPrintFile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:src="@drawable/td_ic_3dprint_modelfile"
        app:layout_constraintBottom_toBottomOf="@+id/vPrintFile"
        app:layout_constraintStart_toStartOf="@+id/vPrintFile"
        app:layout_constraintTop_toTopOf="@+id/vPrintFile" />

    <TextView
        android:id="@+id/tvFileName"
        android:layout_width="@dimen/dp_0"
        app:layout_constraintEnd_toEndOf="@+id/vPrintFile"
        android:layout_marginEnd="@dimen/dp_10"
        android:ellipsize="end"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:text="Future.gcode"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_21"
        app:layout_constraintStart_toEndOf="@+id/ivPrintFile"
        app:layout_constraintTop_toTopOf="@+id/vPrintFile" />

    <TextView
        android:id="@+id/tvFileContent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_15"
        android:text="2021/41/11 11:11 256k"
        android:textSize="@dimen/sp_10"
        app:layout_constraintStart_toEndOf="@+id/ivPrintFile"
        app:layout_constraintTop_toBottomOf="@+id/tvFileName" />

</androidx.constraintlayout.widget.ConstraintLayout>